{
    "name": "utilitte/#NAME#",
    "description": "",
    "license": "proprietary",
    "type": "library",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.33",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^0.12.2",
        "utilitte/qa": "~1.0.0",
        "nette/tester": "^2.3.0"
    },
    "autoload": {
        "psr-4": {
            "Utilitte\\#NAMESPACE#\\": "src/"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@phpstan",
            "@tests"
        ],
        "tests": "vendor/bin/tester tests -C",
        "phpstan": "vendor/bin/phpstan analyse -l 8 -c phpstan.neon src",
        "cs": "vendor/bin/codesniffer",
        "csfix": "vendor/bin/codefixer"
    }
}
